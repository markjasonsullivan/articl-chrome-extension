{
  "manifest_version": 2,

  "name": "Articl",
  "description": "See what your smart friends are reading.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Articl"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}